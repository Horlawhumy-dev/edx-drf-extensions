    "basePath": "/api", 
    "securityDefinitions": {
        "Basic": {
            "type": "basic"
        }
    }, 
    "host": "testserver", 
    "definitions": {
        "Hedgehog": {
            "required": [
                "uuid", 
                "key", 
                "name", 
                "weight_grams", 
                "fav_food"
            ], 
            "type": "object", 
            "properties": {
                "is_college_graduate": {
                    "default": false, 
                    "type": "boolean", 
                    "title": "Is college graduate"
                }, 
                "uuid": {
                    "format": "uuid", 
                    "type": "string", 
                    "title": "Uuid"
                }, 
                "weight_grams": {
                    "type": "integer", 
                    "title": "Weight grams"
                }, 
                "weight_ounces": {
                    "readOnly": true, 
                    "type": "string", 
                    "title": "Weight ounces"
                }, 
                "key": {
                    "pattern": "^[-a-zA-Z0-9_]+$", 
                    "format": "slug", 
                    "type": "string", 
                    "minLength": 1, 
                    "title": "Key"
                }, 
                "fav_food": {
                    "enum": [
                        "critters", 
                        "crawlies", 
                        "strawberries"
                    ], 
                    "type": "string", 
                    "title": "Fav food"
                }, 
                "name": {
                    "minLength": 1, 
                    "type": "string", 
                    "title": "Name"
                }
            }
        }
    }, 
    "security": [
        {
            "Basic": []
